{
  "name": "vulcanize",
  "version": "1.14.11",
  "description": "Process Web Components into one output file",
  "main": "lib/vulcan.js",
  "bin": {
    "vulcanize": "bin/vulcanize"
  },
  "dependencies": {
    "dom5": "^1.3.6",
    "es6-promise": "^2.1.0",
    "mkdirp": "^0.5.1",
    "nopt": "^3.0.1",
    "parse5": "^2.2.1",
    "path-posix": "^1.0.0",
    "polymer-analyzer": "^2.0.0-alpha.4"
  },
  "devDependencies": {
    "@types/chai": "^3.4.30",
    "@types/mocha": "^2.2.29",
    "@types/node": "^6.0.33",
    "chai": "^3.5.0",
    "clang-format": "^1.0.42",
    "eslint": "^2.8.0",
    "firebase": "^2.4.1",
    "mocha": "^2.2.4",
    "source-map-support": "^0.4.2",
    "typescript": "^2.1.0-dev.20160816",
    "typings": "^1.3.2"
  },
  "scripts": {
    "test": "npm run format && tsc && tslint -c tslint.json src/*.ts src/**/*.ts ; mocha",
    "format": "find src | grep '\\.js$\\|\\.ts$' | xargs ./node_modules/.bin/clang-format --style=file -i"
  },
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "web components",
    "polymer"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Polymer/vulcanize.git"
  },
  "bugs": {
    "url": "https://github.com/Polymer/vulcanize/issues"
  }
}
